{
  "manifest_version": 2,
  "name": "Deduplicator",
  "version": "1.0",

  "description": "Closes duplicate tabs",

  "icons": {
    "48": "icons/border-48.png"
  },

  "permissions": ["tabs"],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Deduplicator",
    "default_popup": "deduplicator.html"
  }
}
